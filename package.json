{
  "name": "braekhus",
  "version": "0.0.1",
  "description": "Hole-punching reverse HTTPS proxy",
  "private": true,
  "type": "module",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/express": "4.17.14",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.5",
    "@types/yargs": "^17.0.13",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "jscpd": "^4.0.5",
    "lint-staged": "^15.5.0",
    "pino-pretty": "^10.0.1",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "regenerator-runtime": "^0.13.11",
    "supertest": "^6.3.3",
    "tsconfig-paths": "^4.1.0",
    "tsx": "^4.19.4",
    "typeconv": "^2.1.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "express": "^4.21.2",
    "jose": "^5.10.0",
    "json-rpc-2.0": "^1.7.0",
    "json-stream-stringify": "^3.1.6",
    "lodash-es": "^4.17.21",
    "node-jq": "^6.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "ssl-root-cas": "^1.3.1",
    "ws": "^8.18.1",
    "yargs": "^17.7.2"
  },
  "resolutions": {
    "strip-ansi": "npm:strip-ansi@^6.0.1",
    "string-width": "npm:string-width@^4.2.3",
    "wrap-ansi": "npm:wrap-ansi@^7.0.0"
  },
  "scripts": {
    "build": "yarn clean && tsc --project ./",
    "clean": "rm -rf ./dist",
    "test": "NODE_ENV=test vitest --run",
    "test:pretty": "NODE_ENV=test vitest --run | pino-pretty",
    "start:dev:server": "NODE_ENV=development tsx --watch cli/index.ts server",
    "start:prod:server": "NODE_PATH=dist node dist/cli server",
    "start:dev:client": "NODE_ENV=development tsx --watch cli/index.ts client",
    "start:prod:client": "NODE_PATH=dist node dist/cli client",
    "prepare": "husky install",
    "format": "prettier --write '**/*.{html,js,ts,json,less,md}'",
    "format:quick": "pretty-quick --branch origin/main"
  },
  "lint-staged": {
    "*.(md|yaml|yml|json|js|ts|jsx|tsx)": "prettier --check"
  }
}
