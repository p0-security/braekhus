{
  "name": "kubernetes-remote-server",
  "version": "0.0.1",
  "description": "Kubernetes Remote Server",
  "private": true,
  "devDependencies": {
    "@swc/core": "^1.3.23",
    "@swc/helpers": "^0.4.14",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.18",
    "@types/ws": "^8.5.5",
    "@types/yargs": "^17.0.13",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.2",
    "jest": "29.2.1",
    "jest-extended": "3.2.4",
    "jest-mock": "29.2.1",
    "jscpd": "^3.5.4",
    "lint-staged": "^13.1.0",
    "nodemon": "2.0.20",
    "pino-pretty": "9.1.1",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typeconv": "^2.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "jose": "^4.14.4",
    "json-rpc-2.0": "^1.6.0",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "ws": "^8.13.0",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "build": "yarn clean && tsc --project ./",
    "clean": "rm -rf ./dist",
    "test": "NODE_ENV=test jest --color --testPathIgnorePatterns '.*\\.((integration|e2e)\\.test|snap)\\.tsx?'",
    "test:pretty": "NODE_ENV=test jest --color --testPathIgnorePatterns '.*\\.((integration|e2e)\\.test|snap)\\.tsx?' | pino-pretty",
    "start:dev:server": "NODE_ENV=development nodemon cli server",
    "start:prod:server": "NODE_PATH=dist node dist/cli server",
    "start:dev:client": "NODE_ENV=development nodemon cli client",
    "start:prod:client": "NODE_PATH=dist node dist/cli client",
    "prepare": "cd .. && husky install ts/.husky",
    "format": "prettier --write '**/*.{html,js,ts,json,less,md}'",
    "format:quick": "pretty-quick --branch origin/main"
  },
  "lint-staged": {
    "*.(md|yaml|yml|json|js|ts|jsx|tsx)": "prettier --check"
  }
}
